{
  "name": "clip-bee",
  "preview": true,
  "displayName": "ClipBee",
  "description": "View and paste from clipboard history",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "repository": {
    "url": "https://github.com/nithiinpeter/clip-bee",
    "type": "git",
    "directory": "vs-code-extension"
  },
  "author": {
    "name": "nithinpeter"
  },
  "keywords": [
    "clipboard",
    "clipboard history",
    "copy paste",
    "copy paste history",
    "clipbee"
  ],
  "categories": [
    "Other"
  ],
  "publisher": "vespa-dev-works",
  "galleryBanner": {
    "theme": "dark",
    "color": "#000000"
  },
  "icon": "assets/logo.png",
  "activationEvents": [
    "onCommand:clipbee.showClipboardHistory"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clipbee.showClipboardHistory",
        "title": "Show clipboard history"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.21",
    "@types/node-fetch": "^2.5.2",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
